<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".Main_Helper"
    tools:showIn="@layout/activity_main__helper">

    <Button
        android:id="@+id/button"
        android:layout_width="221dp"
        android:layout_height="61dp"
        android:text="HELP ME"
        android:onClick="makeACall"
        tools:layout_editor_absoluteX="82dp"
        tools:layout_editor_absoluteY="16dp" />

    <CheckBox
        android:id="@+id/phone_cb"
        android:layout_width="92dp"
        android:layout_height="31dp"
        android:onClick="changeCatergory"
        android:text="טלפון"
        tools:layout_editor_absoluteX="82dp"
        tools:layout_editor_absoluteY="164dp" />

    <CheckBox
        android:id="@+id/tv_cb"
        android:layout_width="92dp"
        android:layout_height="35dp"
        android:text="טלוויזיה"
        tools:layout_editor_absoluteX="82dp"
        tools:layout_editor_absoluteY="208dp" />

    <CheckBox
        android:id="@+id/other_cb"
        android:layout_width="94dp"
        android:layout_height="33dp"
        android:onClick="changeCatergory"
        android:text="אחר"
        tools:layout_editor_absoluteX="82dp"
        tools:layout_editor_absoluteY="261dp" />

    <CheckBox
        android:id="@+id/ac_cb"
        android:layout_width="94dp"
        android:layout_height="32dp"
        android:text="מזגן"
        tools:layout_editor_absoluteX="208dp"
        tools:layout_editor_absoluteY="163dp" />

    <CheckBox
        android:id="@+id/pc_cb"
        android:layout_width="94dp"
        android:layout_height="33dp"
        android:text="מחשב"
        tools:layout_editor_absoluteX="209dp"
        tools:layout_editor_absoluteY="211dp" />

    <CheckBox
        android:id="@+id/wm_cb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="מכונת כביסה"
        tools:layout_editor_absoluteX="209dp"
        tools:layout_editor_absoluteY="261dp" />

</android.support.constraint.ConstraintLayout>